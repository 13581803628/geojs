{
  "opts": {
    "template": "jsdoc/template"
  },
  "plugins": [
    "node_modules/jsdoc-autoprivate/autoprivate.js",
    "plugins/markdown"
  ],
  "templates": {
    "applicationName": "GeoJS",
    "linenums": true,
    "cleverLinks": true,
    "default": {
      "outputSourceFiles": true
    }
  }
}
